generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = -1
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid())
  email    String  @unique
  username String?
  gender   String
  age      Int
  role     Role    @relation(fields: [roleId], references: [id])
  roleId   String
}

model Role {
  id          String       @id @default(uuid())
  name        String       @unique
  updatedAt   DateTime     @updatedAt
  User        User[]
}
